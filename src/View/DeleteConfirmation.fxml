<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.DialogPane?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<DialogPane fx:id="dp" prefHeight="240.0" prefWidth="380.0" styleClass="backgrounddialog" stylesheets="@../MyLibrary/Style.css" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.DialogController">
   <content>
      <VBox alignment="TOP_CENTER" fillWidth="false">
         <children>
            <ImageView fitHeight="74.0" fitWidth="74.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../MyLibrary/Icons/iconWarning.png" />
               </image>
               <VBox.margin>
                  <Insets top="10.0" />
               </VBox.margin>
            </ImageView>
            <Label styleClass="labelmediumwhite" stylesheets="@../MyLibrary/Style.css" text="Xác nhận xóa">
               <VBox.margin>
                  <Insets top="14.0" />
               </VBox.margin>
            </Label>
            <HBox fillHeight="false">
               <children>
                  <Button fx:id="btnYesDL" mnemonicParsing="false" onAction="#YesDL" prefHeight="42.0" prefWidth="132.0" styleClass="buttonwhite" stylesheets="@../MyLibrary/Style.css" text="Có">
                     <HBox.margin>
                        <Insets right="37.0" />
                     </HBox.margin>
                  </Button>
                  <Button fx:id="btnNoDL" mnemonicParsing="false" onAction="#NoDL" prefHeight="42.0" prefWidth="132.0" styleClass="buttonmain" stylesheets="@../MyLibrary/Style.css" text="Không" />
               </children>
               <VBox.margin>
                  <Insets top="33.0" />
               </VBox.margin>
            </HBox>
         </children>
      </VBox>
   </content>
</DialogPane>
